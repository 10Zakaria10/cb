{
  "name": "nw-nextjs-boilerplate",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --no-lint",
    "build:export": "next build --no-lint && next export",
    "start": "next start",
    "lint": "next lint --ignore-path .gitignore --dir .",
    "lint:fix": "yarn lint --fix --ext .ts,.tsx,.js,.jsx",
    "commit": "git cz",
    "test": "jest",
    "test:snap": "jest --maxWorkers=50% --silent --watchAll=false --testMatch='!**/*' --testMatch='**/*.snap.tsx'",
    "test:coverage": "jest --coverage --maxWorkers=50% --silent --watchAll=false --reporters=default --coverageReporters=cobertura --coverageReporters=text-summary --coverageReporters=json-summary --coverageReporters=lcov --testMatch='**/*.{spec,test}.{js,jsx,ts,tsx}'",
    "check-unused-exports": "ts-unused-exports tsconfig.json --excludePathsFromReport=pages --ignoreFiles=\"\\.(test|snap|spec)$\"",
    "generate": "plop"
  },
  "dependencies": {
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@hookform/devtools": "^4.1.0",
    "@hookform/resolvers": "^2.8.9",
    "@mui/icons-material": "^5.8.0",
    "@mui/lab": "5.0.0-alpha.86",
    "@mui/material": "^5.10.7",
    "@mui/styled-engine-sc": "^5.10.6",
    "@mui/system": "5.8.0",
    "@mui/x-date-pickers": "5.0.3",
    "@reduxjs/toolkit": "^1.7.1",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "jest-junit": "^13.0.0",
    "lodash": "^4.17.21",
    "next": "12.0.7",
    "next-seo": "^4.28.1",
    "notistack": "^2.0.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.31.1",
    "react-query": "^3.39.0",
    "react-redux": "^7.2.6",
    "react-table": "^7.7.0",
    "styled-components": "^5.3.6",
    "yup": "^0.32.11"
  },
  "prettier": {
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@ginkoe/eslint-config": "^0.1.0",
    "@testing-library/dom": "^8.11.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "17.0.8",
    "@types/react": "17.0.38",
    "@types/react-table": "^7.7.11",
    "@types/styled-components": "^5.1.25",
    "@types/lodash": "^4.14.186",
    "autoprefixer": "^10.4.2",
    "babel-plugin-styled-components": "^2.0.7",
    "commitizen": "^4.2.4",
    "eslint": "^7",
    "eslint-config-next": "12.0.7",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "git-cz": "^4.8.0",
    "jest": "^27.4.7",
    "lint-staged": "^13.0.3",
    "next-http-proxy-middleware": "^1.2.4",
    "plop": "^3.0.5",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-unused-exports": "^8.0.0",
    "typescript": "4.8.2"
  },
  "resolutions": {
    "@types/react": "^17.0.38"
  }
}
